    "animejs": "^3.0.1",
    "aos": "^2.3.4",
    "autoprefixer": "7.2.5",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "bemo-scaffold": "*",
    "bootstrap-italia": "^1.4.1",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
